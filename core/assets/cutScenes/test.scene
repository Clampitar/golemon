
void bar( int q) do
  for(;q > 0;) do
    q--;
    for(var i = 0;i < q; i++) do
    var x = 3;
    var y = 3;
    if((q % 4) % 3 == 0) then
      x = -3;
    end
    if(q % 4 < 2) then
      y = -3;
    end 
    walk(x, y);
    moveCam(0-x, y);
    frameAdvance ;
    end
  end
  return ;
end


 bar(20);